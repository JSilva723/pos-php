{{ form_start(form) }}
<div class="card mb-4">
	<div class="card-header">
		{{ form_header|trans }}
	</div>
	<div class="card-body card-body-c">
        {% block formBody %}{% endblock %}
	</div>
	<div class="card-footer">
		<button class="btn btn-success float-end">
			{{ button_label|trans }}
		</button>
	</div>
</div>
{{ form_end(form) }}
